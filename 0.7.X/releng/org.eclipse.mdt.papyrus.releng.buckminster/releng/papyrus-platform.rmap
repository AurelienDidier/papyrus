	<!-- EMF Query, EMF Validation -->
	<searchPath name="emf.milestones">
		<provider componentTypes="eclipse.feature,osgi.bundle" readerType="p2" source="false" mutable="false">
			<uri format="http://download.eclipse.org/modeling/emf/updates/milestones/" />
		</provider>
	</searchPath>
	
	<!-- EMF Compare -->
	<searchPath name="emf.compare">
		<provider componentTypes="eclipse.feature,osgi.bundle" readerType="p2" source="false" mutable="false">
			<uri format="http://download.eclipse.org/modeling/emf/updates/releases/" />
		</provider>
	</searchPath>

	<!-- EMF -->
	<searchPath name="emf">
		<provider componentTypes="eclipse.feature,osgi.bundle" readerType="p2" source="false" mutable="false">
			<uri format="http://download.eclipse.org/modeling/emf/emf/updates/2.6" />
		</provider>
	</searchPath>

	<!-- ATL -->
	<searchPath name="m2m">
		<provider componentTypes="eclipse.feature,osgi.bundle" readerType="p2" source="false" mutable="false">
			<uri format="http://download.eclipse.org/modeling/m2m/updates/releases/" />
		</provider>
	</searchPath>

	<!-- QVT OML -->
	<searchPath name="qvtoml">
		<provider componentTypes="eclipse.feature,osgi.bundle" readerType="p2" source="false" mutable="false">
			<uri format="http://download.eclipse.org/modeling/m2m/qvtoml/updates/releases/" />
		</provider>
	</searchPath>

	<!-- Acceleo -->
	<searchPath name="m2t">
		<provider componentTypes="eclipse.feature,osgi.bundle" readerType="p2" source="false" mutable="false">
			<uri format="http://download.eclipse.org/modeling/m2t/updates/milestones/" />
		</provider>
	</searchPath>

	<!-- OCL -->
	<searchPath name="ocl">
		<provider componentTypes="eclipse.feature,osgi.bundle" readerType="p2" source="false" mutable="false">
			<uri format="http://download.eclipse.org/modeling/mdt/ocl/3_0/updates/releases/" />
		</provider>
	</searchPath>

