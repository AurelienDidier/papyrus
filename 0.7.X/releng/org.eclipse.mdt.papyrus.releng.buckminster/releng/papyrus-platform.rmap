	<searchPath name="mdt">
		<provider componentTypes="eclipse.feature,osgi.bundle" readerType="p2" source="false" mutable="false">
			<uri format="http://download.eclipse.org/modeling/mdt/updates/releases/" />
		</provider>
	</searchPath>

	<!-- Mylyn (WikiText) -->
	<searchPath name="mylyn">
		<provider componentTypes="eclipse.feature,osgi.bundle" readerType="p2" source="false" mutable="false">
			<uri format="http://download.eclipse.org/tools/mylyn/update/indigo/maintenance" />
		</provider>
	</searchPath>

	<!-- Xtext -->
	<searchPath name="tmf">
		<provider componentTypes="eclipse.feature,osgi.bundle" readerType="p2" source="false" mutable="false">
			<uri format="http://download.eclipse.org/modeling/tmf/updates/" />
		</provider>
	</searchPath>

	<!-- Orbit -->
	<searchPath name="orbit">
		<provider componentTypes="osgi.bundle" readerType="p2" source="false" mutable="false">
			<uri format="http://download.eclipse.org/tools/orbit/downloads/drops/R20100519200754/repository/" />
		</provider>
	</searchPath>
	
	<searchPath name="modisco">
		<provider componentTypes="eclipse.feature,osgi.bundle" readerType="p2" source="false" mutable="false">
			<uri format="{0}/modeling/mdt/modisco/updates/release/0.8.0/">
				<bc:propertyRef key="eclipse.download" />
			</uri>
		</provider>
	</searchPath>
	
	<searchPath name="gef">
		<provider componentTypes="eclipse.feature,osgi.bundle" readerType="p2" source="false" mutable="false">
			<uri format="{0}/tools/gef/updates/releases">
				<bc:propertyRef key="eclipse.download" />
			</uri>
		</provider>
	</searchPath>
	
	<searchPath name="gmf">
		<provider componentTypes="eclipse.feature,osgi.bundle" readerType="p2" source="false" mutable="false">
			<uri format="{0}/modeling/gmp/updates/releases/">
				<bc:propertyRef key="eclipse.download" />
			</uri>
		</provider>
	</searchPath>
	
	<searchPath name="uml2">
		<provider componentTypes="eclipse.feature,osgi.bundle" readerType="p2" source="false" mutable="false">
			<uri format="{0}/modeling/mdt/updates/releases/">
				<bc:propertyRef key="eclipse.download" />
			</uri>
		</provider>
	</searchPath>

	<searchPath name="subversive">
		<provider componentTypes="eclipse.feature,osgi.bundle" readerType="p2" source="false" mutable="false">
			<uri format="{0}/technology/subversive/0.7/update-site">
				<bc:propertyRef key="eclipse.download" />
			</uri>
		</provider>
	</searchPath>

	<searchPath name="platform">
		<provider componentTypes="eclipse.feature,osgi.bundle" readerType="p2" source="false" mutable="false">
			 <uri format="http://download.eclipse.org/eclipse/updates/3.6.x"/>
		</provider>
	</searchPath>

	<property key="checkout.location" value="${workspace.root}/releng/" />
	<property key="eclipse.download" value="http://download.eclipse.org" />
</rmap>
