      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eclipse.papyrus.tests.framework.exceptions.presentation.ExceptionsModelWizardID"
            name="%_UI_ExceptionsModelWizard_label"
