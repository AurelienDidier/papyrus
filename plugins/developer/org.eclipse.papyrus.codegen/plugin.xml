<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="true"
            id="org.eclipse.papyrus.codegen.executeTemplate"
            nameFilter="*.gmfgen"
            objectClass="org.eclipse.core.resources.IFile">
         <action
               class="org.eclipse.papyrus.codegen.actions.PapyrusExecuteTemplateAction"
               enablesFor="1"
               id="org.eclipse.papyrus.codegen.executeTemplateAction"
               label="Generate Papyrus diagram"
               menubarPath="additions">
         </action>
      </objectContribution>
   </extension>
       <extension point="org.eclipse.ui.navigator.viewer">
        <viewerContentBinding viewerId="org.eclipse.papyrus.core.papyrusEditor">
            <includes>
                <contentExtension pattern="org.eclipse.papyrus.core.modelOutline"/>
                <!--contentExtension pattern="org.eclipse.papyrus.core.viewOutline"/-->
            </includes>
        </viewerContentBinding>
    </extension>
   

</plugin>
