--- src/org/eclipse/papyrus/diagram/common/util/UMLPropertyTester.java	(revision 0)
+++ src/org/eclipse/papyrus/diagram/common/util/UMLPropertyTester.java	(revision 0)
@@ -0,0 +1,35 @@
+package org.eclipse.papyrus.diagram.common.util;
+
+import org.eclipse.core.expressions.PropertyTester;
+import org.eclipse.uml2.uml.Element;
+
+
+public class UMLPropertyTester extends PropertyTester {
+
+	public static String pNamespace = "org.eclipse.papyrus.uml.";
+
+	public static String pIsStereotyped = "isStereotyped";
+
+	public UMLPropertyTester() {
+
+	}
+
+	public boolean test(Object receiver, String property, Object[] args, Object expectedValue) {
+
+		Element element = (Element)receiver;
+
+		if(pIsStereotyped.equals(property)) {
+			boolean allStereotypeApplied = false;
+			for(Object arg : args) {
+				if(element.getAppliedStereotype((String)arg) != null) {
+					return true;
+				}
+			}
+
+			return allStereotypeApplied;
+		}
+
+		return false;
+	}
+
+}
