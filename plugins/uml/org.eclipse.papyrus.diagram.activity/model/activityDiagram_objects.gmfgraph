<?xml version="1.0" encoding="UTF-8"?>
<gmfgraph:Canvas xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmfgraph="http://www.eclipse.org/gmf/2006/GraphicalDefinition">
  <figures>
    <descriptors
        name="CentralBufferFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle"
          name="CentralBufferFigure">
        <layout
            xsi:type="gmfgraph:BorderLayout"/>
        <children
            xsi:type="gmfgraph:Label"
            name="CentralBufferFigure_fixed_central"
            text="\u00ABcentralBuffer\u00BB">
          <layoutData
              xsi:type="gmfgraph:BorderLayoutData"
              alignment="BEGINNING"
              vertical="true"/>
        </children>
        <children
            xsi:type="gmfgraph:Rectangle"
            name="CentralBufferFigure_ContentPane"
            outline="false"
            fill="false">
          <layoutData
              xsi:type="gmfgraph:BorderLayoutData"
              alignment="FILL"/>
        </children>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.0/@actualFigure/@children.1"/>
    </descriptors>
    <descriptors
        name="DataStoreFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle"
          name="DataStoreFigure">
        <layout
            xsi:type="gmfgraph:GridLayout"/>
        <foregroundColor
            xsi:type="gmfgraph:ConstantColor"
            value="black"/>
        <minimumSize
            dx="160"
            dy="60"/>
        <children
            xsi:type="gmfgraph:Label"
            name="DataStore_fixed_datastore"
            text="\u00ABdatastore\u00BB">
          <layoutData
              xsi:type="gmfgraph:GridLayoutData"/>
          <font
              xsi:type="gmfgraph:BasicFont"
              faceName="SANS"/>
          <insets
              top="2"
              bottom="2"/>
        </children>
        <children
            xsi:type="gmfgraph:Label"
            name="DataStoreFigure_name">
          <layoutData
              xsi:type="gmfgraph:GridLayoutData"
              grabExcessHorizontalSpace="true"
              grabExcessVerticalSpace="true"/>
          <font
              xsi:type="gmfgraph:BasicFont"
              faceName="SANS"
              height="10"
              style="BOLD"/>
          <insets
              top="2"
              bottom="2"/>
        </children>
      </actualFigure>
      <accessors
          accessor="getFigureDataStoreFigure_name"
          figure="//@figures.0/@descriptors.1/@actualFigure/@children.1"/>
    </descriptors>
    <descriptors
        name="StandalonePinFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle"
          name="StandalonePinFigure">
        <layout
            xsi:type="gmfgraph:BorderLayout"/>
        <foregroundColor
            xsi:type="gmfgraph:ConstantColor"
            value="black"/>
        <minimumSize
            dx="40"
            dy="40"/>
        <children
            xsi:type="gmfgraph:Label"
            name="StandalonePinFigure_name">
          <layoutData
              xsi:type="gmfgraph:BorderLayoutData"
              vertical="true"/>
          <font
              xsi:type="gmfgraph:BasicFont"
              faceName="SANS"
              height="10"
              style="BOLD"/>
          <insets
              top="2"
              left="5"
              bottom="2"
              right="5"/>
        </children>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.2/@actualFigure/@children.0"/>
    </descriptors>
    <descriptors
        name="SmallSquareFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle"
          name="SmallSquareFigure">
        <layout
            xsi:type="gmfgraph:StackLayout"/>
        <foregroundColor
            xsi:type="gmfgraph:ConstantColor"
            value="black"/>
        <backgroundColor
            xsi:type="gmfgraph:RGBColor"
            red="255"
            green="215"
            blue="174"/>
        <maximumSize
            dx="15"
            dy="15"/>
        <minimumSize
            dx="15"
            dy="15"/>
        <preferredSize
            dx="15"
            dy="15"/>
        <children
            xsi:type="gmfgraph:CustomFigure"
            name="pinArrowFigure"
            qualifiedClassName="es.cv.gvcase.mdt.uml2.diagram.activity.draw2d.PinArrowFigure"/>
      </actualFigure>
    </descriptors>
    <descriptors
        name="AttachedPin_external_name">
      <actualFigure
          xsi:type="gmfgraph:Label"
          name="AttachedPin_external_name">
        <font
            xsi:type="gmfgraph:BasicFont"
            faceName="SANS"
            height="10"
            style="BOLD"/>
      </actualFigure>
    </descriptors>
    <descriptors
        name="ActivityParameterFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle"
          name="ActivityParameterFigure">
        <layoutData
            xsi:type="gmfgraph:CustomLayoutData"
            qualifiedClassName="es.cv.gvcase.mdt.uml2.diagram.common.draw2d.CenterLayout"/>
        <foregroundColor
            xsi:type="gmfgraph:ConstantColor"
            value="black"/>
        <children
            xsi:type="gmfgraph:Label"
            name="ActivityParameterFigure_name">
          <font
              xsi:type="gmfgraph:BasicFont"
              faceName="SANS"
              height="10"
              style="BOLD"/>
          <border
              xsi:type="gmfgraph:MarginBorder">
            <insets
                top="3"
                left="5"
                bottom="3"
                right="5"/>
          </border>
        </children>
        <children
            xsi:type="gmfgraph:Rectangle"
            name="ActivityParameterFigure">
          <layoutData
              xsi:type="gmfgraph:CustomLayoutData"
              qualifiedClassName="es.cv.gvcase.mdt.uml2.diagram.common.draw2d.CenterLayout"/>
          <foregroundColor
              xsi:type="gmfgraph:ConstantColor"
              value="black"/>
          <children
              xsi:type="gmfgraph:Label"
              name="ActivityParameterFigure_name">
            <border
                xsi:type="gmfgraph:MarginBorder">
              <insets
                  top="3"
                  left="5"
                  bottom="3"
                  right="5"/>
            </border>
          </children>
        </children>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.5/@actualFigure/@children.0"/>
    </descriptors>
  </figures>
  <nodes
      name="DataStoreNode"
      figure="DataStoreFigure">
    <facets
        xsi:type="gmfgraph:DefaultSizeFacet">
      <defaultSize
          dx="160"
          dy="60"/>
    </facets>
  </nodes>
  <nodes
      name="CentralBufferNode"
      figure="CentralBufferFigure"/>
  <nodes
      name="StandalonePinNode"
      figure="StandalonePinFigure">
    <facets
        xsi:type="gmfgraph:DefaultSizeFacet">
      <defaultSize
          dx="40"
          dy="40"/>
    </facets>
  </nodes>
  <nodes
      name="AttachedOutputPinNode"
      figure="SmallSquareFigure"
      resizeConstraint="NONE"
      affixedParentSide="EAST"/>
  <nodes
      name="AttachedInputPinNode"
      figure="SmallSquareFigure"
      resizeConstraint="NONE"
      affixedParentSide="WEST"/>
  <nodes
      name="ActivityParameterNode"
      figure="ActivityParameterFigure"
      resizeConstraint="NONE"
      affixedParentSide="WEST">
    <facets
        xsi:type="gmfgraph:DefaultSizeFacet">
      <defaultSize
          dx="70"
          dy="25"/>
    </facets>
  </nodes>
  <compartments
      name="CentralBufferNode_ContentPane"
      figure="CentralBufferFigure"
      accessor="//@figures.0/@descriptors.0/@accessors.0"/>
  <labels
      name="StandalonePinNode_name"
      figure="StandalonePinFigure"
      elementIcon="false"
      accessor="//@figures.0/@descriptors.2/@accessors.0"/>
  <labels
      name="AttachedPinNode_external_name"
      figure="AttachedPin_external_name"
      elementIcon="false"/>
  <labels
      name="ActivityParameterNode_name"
      figure="ActivityParameterFigure"
      accessor="//@figures.0/@descriptors.5/@accessors.0"/>
  <labels
      name="DataStoreName"
      figure="DataStoreFigure"
      elementIcon="false"
      accessor="//@figures.0/@descriptors.1/@accessors.0"/>
</gmfgraph:Canvas>
