<?xml version="1.0" encoding="UTF-8"?>
<gmfgraph:Canvas xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmfgraph="http://www.eclipse.org/gmf/2006/GraphicalDefinition">
  <figures
      name="Default">
    <descriptors
        name="ActivityPartitionFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle"
          name="ActivityPartitionFigure">
        <layout
            xsi:type="gmfgraph:BorderLayout"/>
        <foregroundColor
            xsi:type="gmfgraph:ConstantColor"
            value="black"/>
        <children
            xsi:type="gmfgraph:Rectangle"
            name="ActivityNameRectangle">
          <layoutData
              xsi:type="gmfgraph:BorderLayoutData"
              alignment="BEGINNING"
              vertical="true"/>
          <layout
              xsi:type="gmfgraph:StackLayout"/>
          <foregroundColor
              xsi:type="gmfgraph:ConstantColor"
              value="black"/>
          <children
              xsi:type="gmfgraph:Label"
              name="ActivityPartitionFigure_name">
            <font
                xsi:type="gmfgraph:BasicFont"
                faceName="SANS"
                height="10"
                style="BOLD"/>
            <insets
                top="5"
                left="5"
                bottom="5"
                right="5"/>
          </children>
        </children>
        <children
            xsi:type="gmfgraph:Rectangle"
            name="ActivityPartitionBody">
          <layoutData
              xsi:type="gmfgraph:BorderLayoutData"
              alignment="FILL"/>
        </children>
      </actualFigure>
      <accessors
          accessor="getFigureActivityPartitionFigure_name"
          figure="//@figures.0/@descriptors.0/@actualFigure/@children.0/@children.0"/>
      <accessors
          accessor="getFigureActivityPartitionFigure_body"
          figure="//@figures.0/@descriptors.0/@actualFigure/@children.1"/>
    </descriptors>
  </figures>
  <nodes
      name="ActivityPartition"
      figure="ActivityPartitionFigure">
    <facets
        xsi:type="gmfgraph:DefaultSizeFacet">
      <defaultSize
          dx="150"
          dy="400"/>
    </facets>
  </nodes>
  <compartments
      name="ActivityPartitionCompartment"
      figure="ActivityPartitionFigure"
      accessor="//@figures.0/@descriptors.0/@accessors.1"/>
  <labels
      name="ActivityPartitionName"
      figure="ActivityPartitionFigure"
      accessor="//@figures.0/@descriptors.0/@accessors.0"/>
</gmfgraph:Canvas>
