      <extension point="org.eclipse.ui.views">
   <category 
	id="org.eclipse.papyrus.views.category"
	name="Papyrus">
   </category>
   
</extension>
<extension
            point="org.eclipse.gmf.runtime.common.ui.services.action.contributionItemProviders">
         <contributionItemProvider
               checkPluginLoaded="true"
               class="org.eclipse.papyrus.diagram.common.actions.FilterViewsLabelItemProvider">
            <Priority
                  name="Lowest">
            </Priority>
            <popupContribution
                  class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContextMenuProvider">
               <popupMenuGroup
                     id="FilterViewsGroup"
                     path="/propertiesGroup">
               </popupMenuGroup>
               <popupStructuredContributionCriteria
                     policyClass="org.eclipse.papyrus.diagram.common.actions.FilterViewsLabelsPopupMenuContributionPolicy">
               </popupStructuredContributionCriteria>
               <popupAction
                     id="filter_views_and_labels"
                     path="/FilterViewsGroup">
               </popupAction>
            </popupContribution>
         </contributionItemProvider>
      </extension>
<extension
      point="org.eclipse.papyrus.core.modelListener">
   <listener
         name="StereotypeApplicationListener"
         realization="org.eclipse.papyrus.diagram.common.editparts.PapyrusStereotypeListener">
   </listener>
</extension>
<extension
      point="org.eclipse.ui.newWizards">
   <wizard
         category="org.eclipse.papyrus.wizards.category"
         class="org.eclipse.papyrus.diagram.common.wizards.CreateUMLModelWizard"
         finalPerspective="org.eclipse.papyrus.core.perspective"
         icon="icons/papyrus/PapyrusLogo16x16.gif"
         id="org.eclipse.papyrus.wizards.createumlmodel"
         name="Papyrus Model">
      <selection
            class="org.eclipse.core.resources.IResource">
      </selection>
   </wizard>
</extension>
<extension
      point="org.eclipse.gmf.runtime.diagram.ui.paletteProviders">
   <paletteProvider
         class="org.eclipse.gmf.runtime.diagram.ui.providers.DefaultPaletteProvider">
      <Priority
            name="Highest">
      </Priority>
      <contribution>
         <predefinedEntry
               id="standardGroup/noteStack/noteTool"
               remove="true">
         </predefinedEntry>
         <predefinedEntry
               id="standardGroup/noteStack/textTool"
               remove="true">
         </predefinedEntry>
         <predefinedEntry
               id="standardGroup/noteStack/noteattachmentTool"
               remove="true">
         </predefinedEntry>
      </contribution>
   </paletteProvider>
</extension>

<extension
	point="org.eclipse.ui.views.properties.tabbed.propertySections">
	<propertySections contributorId="TreeOutlinePage">
		<propertySection
			afterSection=""
	   		class="org.eclipse.papyrus.diagram.common.sheet.UMLPropertySection"
	  		id="AdvancedProperty"
	   		tab="advanced">
	   		<input type="java.lang.Object"/>
		</propertySection>
	</propertySections>
</extension>
