         name="RoutingCommand">
      <commandParameter
            id="org.eclipse.papyrus.diagram.common.commandRoutingParameter"
            name="routingParameter"
            optional="true">
      </commandParameter>
   </command>
   <command
         categoryId="org.eclipse.papyrus.editor.category"
         defaultHandler="org.eclipse.papyrus.diagram.common.handlers.DeleteFromModelHandler"
         description="Call the delete from model action"
         id="org.eclipse.papyrus.diagram.common.commands.DeleteFromModelCommand"
         name="Delete From Model Command">
   </command>
