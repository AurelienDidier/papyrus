<?xml version="1.0" encoding="UTF-8"?>
<gmfmap:Mapping xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="UML.ecore#//Package/packagedElement"/>
    <ownedChild
        relatedDiagrams="//@diagram">
      <domainMetaElement
          href="UML.ecore#//Class"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping"
          readOnly="true"
          viewPattern="&lt;&lt;{0}>>"
          editorPattern="&lt;&lt;{0}>>"
          editPattern="&lt;&lt;{0}>>">
        <diagramLabel
            href="../../org.eclipse.papyrus.diagram.common/model/Classifier.gmfgraph#ClassifierStereotypeDiagramLabel"/>
        <features
            href="UML.ecore#//NamedElement/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="classdiagram.gmftool#//@palette/@tools.2/@tools.0"/>
      <diagramNode
          href="../../org.eclipse.papyrus.diagram.common/model/Classifier.gmfgraph#ClassifierNode"/>
    </ownedChild>
  </nodes>
  <links
      relatedDiagrams="//@diagram">
    <domainMetaElement
        href="UML.ecore#//Dependency"/>
    <domainInitializer
        xsi:type="gmfmap:FeatureSeqInitializer">
      <initializers
          xsi:type="gmfmap:FeatureValueSpec">
        <feature
            xsi:type="ecore:EAttribute"
            href="UML.ecore#//NamedElement/name"/>
        <value
            body="NodeClassHelper.EINSTANCE.getnewName()"
            language="java"/>
      </initializers>
    </domainInitializer>
    <labelMappings
        xsi:type="gmfmap:FeatureLabelMapping"
        editPattern="">
      <diagramLabel
          href="../../org.eclipse.papyrus.diagram.common/model/Dependency.gmfgraph#DependencyNameDiagramLabel"/>
      <features
          href="UML.ecore#//NamedElement/name"/>
    </labelMappings>
    <containmentFeature
        href="UML.ecore#//Package/packagedElement"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="classdiagram.gmftool#//@palette/@tools.3/@tools.3"/>
    <diagramLink
        href="../../org.eclipse.papyrus.diagram.common/model/Dependency.gmfgraph#DependencyConnection"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="UML.ecore#//Dependency/client"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="UML.ecore#//Dependency/supplier"/>
  </links>
  <diagram>
    <diagramCanvas
        href="../../org.eclipse.papyrus.diagram.common/model/PackageElement.gmfgraph#umlclassdiagram"/>
    <domainModel
        href="UML.ecore#/"/>
    <domainMetaElement
        href="UML.ecore#//Package"/>
    <palette
        href="classdiagram.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
