<plugin>
   <extension
         point="org.eclipse.papyrus.uml.extensionpoints.UMLProfile">
      <profile
            description="Profile used to identify UML-RT-like &quot;Capsules&quot; in demonstration of model-to-model synchronization with the Active Operations Framework"
            name="UML Synchronization Example Profile"
            path="profiles/sync-example.profile.uml"
            provider="Eclipse Modeling Project">
      </profile>
   </extension>
   <extension
         point="org.eclipse.papyrus.sync.syncDelegates">
      <syncDelegate
            class="org.eclipse.papyrus.aof.sync.examples.uml.internal.CapsuleSyncDelegate"
            priority="50">
      </syncDelegate>
   </extension>

</plugin>
