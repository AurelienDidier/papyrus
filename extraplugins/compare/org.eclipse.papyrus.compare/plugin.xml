<plugin>
  <extension
         point="org.eclipse.emf.compare.match.engine">
      <matchengine
            engineClass="org.eclipse.papyrus.compare.PapyrusMatchEngine"
            fileExtension="uml"
            label="Papyrus Matching Engine"
            priority="highest"/>
   </extension>
